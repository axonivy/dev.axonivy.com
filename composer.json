{
  "require": {
    "slim/slim": "*",
    "slim/twig-view": "*",
    "slim/psr7": "*",
    "php-di/php-di": "*",
    "middlewares/trailing-slash": "*",
    "league/commonmark": "*",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "cyclonedx/cyclonedx-php-composer": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "app\\": "src/app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "cyclonedx/cyclonedx-php-composer": true
    }
  }
}
