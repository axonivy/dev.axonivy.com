						"version": "${version}",
						"type": "iar"
					},
					{
						"groupId": "com.axonivy.portal",
						"artifactId": "portal-developer-examples",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories":[
					{
						"id":"axonivy.public",
						"url":"http://repo.axonivy.rocks"
					}
				]
			}
