{
	"id": "doc-factory",
	"version": "9.3",
	"name": "Doc Factory",
	"description": "Generate documents",
	"sort": 300,
	"versionDisplay": "hide-snapshots",
	"type": "process",
	"tags": ["document"],
	"vendor": "Axon Ivy AG",
	"platformReview": "4.5",
	"cost": "free",
	"language": "en",
	"industry": "Cross-Industry",
	"compatibility": "8.0+",
	"mavenArtifacts": [
		{
			"name": "Doc Factory",
			"groupId": "ch.ivyteam.ivy.addons",
			"artifactId": "doc-factory",
			"makesSenseAsMavenDependency": true
		},
		{
			"name": "Doc Factory Demo",
			"groupId": "ch.ivyteam.ivy.addons",
			"artifactId": "doc-factory-demos"
		},
		{
			"name": "Doc Factory Documentation",
			"groupId": "ch.ivyteam.ivy.addons",
			"artifactId": "doc-factory-doc",
			"type": "zip",
			"doc": true
		}
	],
	"installers": [
		{
			"id": "maven-dependency",
			"data": {
				"dependencies": [
					{
						"groupId": "ch.ivyteam.ivy.addons",
						"artifactId": "doc-factory",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories":[
					{
						"id":"axonivy.public",
						"url":"http://repo.axonivy.rocks",
						"snapshots":{"enabled":"true"}
					}
				]
			}
		},
		{
			"id": "maven-import",
			"data": {
				"projects":[
					{
						"groupId": "ch.ivyteam.ivy.addons",
						"artifactId": "doc-factory-demos",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories":[
					{
						"id":"axonivy.public",
						"url":"http://repo.axonivy.rocks"
					}
				]
			}
		},
		{
			"id": "open-resource",
			"data": {
				"project": "DocFactoryDemos",
				"resources": [
					"processes/DocFactorySubprocessExamples.mod"
				]
			}
		}
	]
}
