{% extends "_parts/base.twig" %}

{% block title %}Search{{ parent() }}{% endblock %}

{% block content %}

<style>
table {
	border-collapse: separate;
	margin: 0 0 0.5em 0;
}

table td {
	padding: 0px;
}

.cse .gsc-search-button-v2, .gsc-search-button-v2 {
	font-size: 8.9px;
	padding: 0px 23.4px;
}

input[type="text"], input[type="password"], input[type="email"], select
	{
	height: 2.75em;
	font-size: 13px;
	line-height: 18px;
}

table.gsc-search-box td {
	line-height: 18px;
}

.gsc-input-box {
}

input[type="text"]:focus, input[type="password"]:focus, input[type="email"]:focus,
	select:focus, textarea:focus {
	box-shadow: 0 0 0 0px #C7D426;
}

.gsc-result .gs-title {
	height: 1.8em;
}
</style>

<section id="main">
	<div class="inner">
		<header>
			<h1>
				<span class="icon fas fa-search"></span> Search
			</h1>
		</header>
		<script async="true" src="https://cse.google.com/cse.js?cx=005882002217446056453:nqlyfqggbhx"></script>
		<div class="gcse-search"></div>
	</div>
</section>

{% endblock %}
